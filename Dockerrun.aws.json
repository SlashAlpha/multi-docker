{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions": [
      {
        "name":"client",
        "image":"slashuzero/multi-client",
        "hostname":"client",
        "essential":false
    },
    {
        "name":"server",
        "image":"slashuzero/multi-server",
        "hostname":"api",
        "essential":false
    },
    {
        "name":"worker",
        "image":"slashuzero/multi-worker",
        "hostname":"worker",
        "essential":false
    },
    {
        "name":"nginx",
        "image":"slashuzero/multi-nginx",
        "hostname":"nginx",
        "essential":true,
        "portMapping": [{
            "hostPort": 80,
            "containerPort": 80
        }],
        "links": ["client", "server"]
    }
    ]
}